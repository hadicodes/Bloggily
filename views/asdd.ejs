
    <h1>Index Page</h1>
    
        <% blogs.forEach(function(blog){ %>
            <div>
                <h2>
                    <%=blog.title%>
                </h2>
                <img src="<%= blog.image %>">
                <p>
                    <%= blog.created %>
                </p>
                <p>
                    <%= blog.body %>
                </p>
            </div>
    
            <% }) %>
