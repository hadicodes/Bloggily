<% include  ./partials/header %>

    <h1 class="container">Blog Index</h1>
    
        <% blogs.forEach(function(blog){ %>
            <div>
                <h2>
                    <%=blog.title%>
                </h2>
                <img src="<%= blog.image %>">
                <p>
                    <%= blog.created %>
                </p>
                <p>
                    <%= blog.body %>
                </p>
            </div>
    
            <% }) %>

            <% include  ./partials/footer %>            